with "config";
with "utilada_core";
with "utilada_base";
with "utilada_sys";
with "utilada_http";
@WITH_UTILADA_LZMA@

abstract project UtilAda is
   Version := "@UTIL_VERSION@";
   Library_Dir := "lib";

   Has_Lzma    : Config.Yes_No := "@UTIL_HAVE_LZMA@";
   Has_Curl    : Config.Yes_No := "@UTIL_HAVE_CURL@";
   Has_AWS     : Config.Yes_No := "@UTIL_HAVE_AWS@";

   --  Enable the compilation of Util.Serialize.Mappers.Vector_Mappers.
   Has_Vector_Mappers : Config.Yes_No := "@UTIL_HAVE_VECTOR_MAPPERS@";

end UtilAda;
